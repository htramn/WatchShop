@model List<WatchShop.Models.CartItem>

@{
    var soluong = 0;
    foreach (var item in Model)
    {
        soluong += item.Quantity;
    }
}

<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti js-show-cart" data-notify="@soluong">

    <a class="cl2 hov-cl1" href="@Url.Action("Index","Cart")"> <i class="zmdi zmdi-shopping-cart"></i></a>
</div>
